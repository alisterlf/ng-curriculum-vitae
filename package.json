{
  "name": "ng-curriculum-vitae",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng build --prod --verbose",
    "deploy": "ng deploy --repo=git@github.com:alisterlf/alisterlf.github.io.git --branch master",
    "e2e": "ng e2e",
    "postinstall": "npm run prettier -s && ivy-ngcc",
    "lint": "ng lint",
    "ng": "ng",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,json,scss}\"",
    "start": "ng serve --aot",
    "test": "ng test"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ],
    "singleQuote": true
  },
  "dependencies": {
    "@angular/animations": "~8.2.4",
    "@angular/cdk": "~8.1.4",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/forms": "~8.2.4",
    "@angular/material": "^8.1.4",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/router": "~8.2.4",
    "core-js": ">=2.5.6 and <3.0.0",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "webfontloader": "^1.6.28",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.2",
    "@angular/cli": "~8.3.2",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~12.7.3",
    "angular-cli-ghpages": "^0.6.0",
    "codelyzer": "^5.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "^1.18.2",
    "protractor": "~5.4.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": ">=3.1 < 3.6"
  }
}
